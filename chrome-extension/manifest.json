{
	"name": "TabulaTabs!",
	"version": "0.1",
	"description": "Access your Browser tabs from everywhere",
	"permissions": [
		"tabs", "http://*/", "https://*/",
		"http://*.tabulatabs.com/"
	],
	"content_scripts": [{
		"js": ["js/findArticleImage.js"],
		"matches": ["http://*/*"]
	}],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"background_page": "backgroundPage.html"
}
