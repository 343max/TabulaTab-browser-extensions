{
   "options_page" : "options.html",
   "permissions" : [
      "tabs",
      "https://tabulatabs.heroku.com/",
      "http://tabulatabs.heroku.com/",
      "chrome://favicon/",
      "http://*/*",
      "https://*/*"
   ],
   "background_page" : "global.html",
   "version" : "15",
   "update_url" : "http://tabulatabs.com/chrome-extension-updates.xml",
   "name" : "TabulaTabs!",
   "description" : "Access your Browser tabs from everywhere",
   "content_scripts" : [
      {
         "matches" : [
            "http://*/*"
         ],
         "js" : [
            "js/content-script.js"
         ]
      }
   ],
   "browser_action" : {
      "default_popup" : "sync-popover-chrome.html",
      "default_icon" : "icon-chrome.png"
   }
}

